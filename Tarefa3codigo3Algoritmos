#include <stdio.h>

int fibonacci(int n) {
    if (n <= 0) return 0;
    else if (n == 1) return 1;
    else {
        int a = 0, b = 1, temp;
        for (int i = 2; i <= n; i++) {
            temp = b;
            b = a + b;
            a = temp;
        }
        return b;
    }
}

int main() {
    int n;
    printf("Digite o termo da série de Fibonacci que deseja calcular: ");
    scanf("%d", &n);
    int resultado = fibonacci(n);
    printf("O %d-ésimo termo da série de Fibonacci é: %d\n", n, resultado);
    return 0;
}

