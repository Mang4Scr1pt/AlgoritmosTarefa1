//Henrique Mangueira
#include <stdio.h>

void exibir Inversa(int vetor [], int tamanho) {
for (int i = tamanho - 1; i >= 0; i--) {
printf("%d", vetor [i]);
}
}
int main() {
int tamanho = 10; 
int vetor [tamanho];

printf("Digite %d números inteiros:\n", tamanho);
for (int i = 0; i < tamanho; i++) {
scanf ("%d", &vetor [i]);
}

printf ("Números na ordem inversa: \n");
exibirInversa(vetor, tamanho);
return 0;
}
